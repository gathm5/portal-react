{
  "name": "kme-portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "animate.css": "^3.5.2",
    "axios": "^0.16.0",
    "better-react-spinkit": "^2.0.0-6",
    "bootstrap": "4.0.0-alpha.6",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "node-sass": "^4.5.1",
    "npm-run-all": "^4.0.2",
    "react": "^15.4.2",
    "react-addons-css-transition-group": "^15.4.2",
    "react-addons-transition-group": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.5.0",
    "react-input-enhancements": "^0.5.3",
    "react-redux": "^5.0.3",
